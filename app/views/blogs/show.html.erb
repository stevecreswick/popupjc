<div class="individual-post">
  <div class="parallax-container">
    <div class="parallax">
      <img src="<%= @blog.header_image %>" alt="" />
    </div>
  </div>
  <h1><%= @blog.title %> </h1>
  <h6><%= @blog.created_at.strftime("%B %d, %Y") %> </h6>
  <h6>By Liina Edun</h6>
  <div class="section">
    <p>
      <%= @blog.message.html_safe %>
    </p>
  </div>
</div>
